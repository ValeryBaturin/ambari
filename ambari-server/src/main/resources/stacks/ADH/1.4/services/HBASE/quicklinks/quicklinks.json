{
  "name": "default",
  "description": "default quick links configuration",
  "configuration": {
    "protocol":
    {
      "type":"https"
    },

    "links": [
      {
        "name": "hbase_master_ui",
        "label": "HBase Master UI",
        "url":"%@://%@:%@/master-status",
        "requires_user_name": "false",
        "port":{
          "http_property": "hbase.master.info.port",
          "http_default_port": "16010",
          "https_property": "hbase.master.info.port",
          "https_default_port": "16010",
          "regex": "",
          "site": "master-hbase-site"
        }
      },
      {
        "name": "hbase_regionserver_ui",
        "label": "HBase Region Server",
        "url":"%@://%@:%@/rs-status",
        "requires_user_name": "false",
        "port":{
          "http_property": "hbase.regionserver.info.port",
          "http_default_port": "16030",
          "https_property": "hbase.regionserver.info.port",
          "https_default_port": "16030",
          "regex": "",
          "site": "region-hbase-site"
        }
      }
    ]
  }
}
