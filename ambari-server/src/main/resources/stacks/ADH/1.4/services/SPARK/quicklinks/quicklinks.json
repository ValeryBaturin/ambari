{
  "name": "default",
  "description": "default quick links configuration",
  "configuration": {
    "protocol":
    {
      "type":"HTTP_ONLY"
    },

    "links": [
      {
        "name": "spark2_history_server_ui",
        "label": "Spark2 History Server UI",
        "component_name": "SPARK2_JOBHISTORYSERVER",
        "requires_user_name": "false",
        "url": "%@://%@:%@",
        "port":{
          "http_property": "spark.history.ui.port",
          "http_default_port": "18085",
          "https_property": "spark.history.ui.port",
          "https_default_port": "18085",
          "regex": "^(\\d+)$",
          "site": "spark2-defaults"
        }
      },
      {
        "name": "spark2_master_ui",
        "label": "Spark2 WEB UI",
        "component_name": "SPARK2_JOBHISTORYSERVER",
        "requires_user_name": "false",
        "url": "%@://%@:%@",
        "port":{
          "http_property": "spark.history.ui.port",
          "http_default_port": "8082",
          "https_property": "spark.history.ui.port",
          "https_default_port": "8082",
          "regex": "^(\\d+)$",
          "site": "spark2-web-ui"
        }
      }
    ]
  }
}
